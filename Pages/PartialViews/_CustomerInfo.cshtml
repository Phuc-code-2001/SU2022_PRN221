@using ShinyTeeth.Models;



<div class="card" style="min-width: 350px">
	<div class="card-body">
		<h4>Customer Infomation :</h4>
		<hr />
		<p>
			<h5>FullName: </h5>
			<i> - @Model.User.FullName</i>
		</p>
		<p>
			<h5>Address: </h5>
			<i> - @(Model.Address ?? "Unset")</i>
		</p>
		<p>
			<h5>Created: </h5>
			<i> - @Model.Created.ToLocalTime()</i>
		</p>
	</div>
</div>